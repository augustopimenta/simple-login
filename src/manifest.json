{
  "name": "Simple Login",
  "description": "Fa√ßa login rapidamente",
  "version": "1.3",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Simple Login",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": []
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": []
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "incognito": "split",
  "icons": { 
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
   "128": "icons/icon128.png" 
  }
}